# Environment Variables for Exonov Quantum

# Database
POSTGRES_URL="postgresql://user:password@localhost:5432/exonov_quantum"

# Authentication
AUTH_SECRET="your-super-secret-auth-key-here"

# Stripe
STRIPE_SECRET_KEY="sk_test_your_stripe_secret_key"
STRIPE_WEBHOOK_SECRET="whsec_your_webhook_secret"

# n8n Integration
N8N_BASE_URL="https://exonov-u39090.vm.elestio.app"
N8N_API_KEY="your-n8n-api-key"
N8N_WEBHOOK_SECRET="your-webhook-secret"

# Quantum Providers
IBM_QUANTUM_TOKEN="your-ibm-quantum-token"
AWS_ACCESS_KEY_ID="your-aws-access-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret-key"
AZURE_SUBSCRIPTION_ID="your-azure-subscription"
GOOGLE_QUANTUM_API_KEY="your-google-quantum-key"

# App Configuration
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NEXT_PUBLIC_DASHBOARD_REFRESH_INTERVAL="30000"
NEXT_PUBLIC_ENABLE_REAL_TIME="true"

# Optional: Analytics
NEXT_PUBLIC_POSTHOG_KEY="your-posthog-key"
NEXT_PUBLIC_POSTHOG_HOST="https://app.posthog.com"
